    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>asd.ico</ApplicationIcon>
    <CosturaUnmanaged32Assemblies></CosturaUnmanaged32Assemblies>
